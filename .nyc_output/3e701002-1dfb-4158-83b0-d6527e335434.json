this.Num1,this.Num2);\r\n            \r\n        }\r\n        else if( Oper == \"dividir\"){\r\n            this.Num1 = traducir.Traductir(this.Num1);\r\n            this.Num2 = traducir.Traductir(this.Num2);\r\n            return calculator.Dividir(this.Num1,this.Num2);\r\n        }\r\n        else{\r\n            if( this.Num1 == \"suma\" || this.Num2 == \"suma\"){\r\n                return console.log(\"Syntax Error\")\r\n            }\r\n            else if( this.Num1 == \"resta\" || this.Num2 == \"resta\"){\r\n                return console.log(\"Syntax Error\")\r\n    \r\n            }\r\n            else if( this.Num1 == \"multiplicar\" || this.Num2 == \"multiplicar\"){\r\n                return console.log(\"Syntax Error\")\r\n                \r\n            }\r\n            else if( this.Num1 == \"dividir\" || this.Num2 == \"dividir\"){\r\n                return console.log(\"Syntax Error\")\r\n            }\r\n        }\r\n        \r\n\r\n            \r\n    }\r\n\r\n}"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"f9733db2234b351de4804bf07d33c41ef980de47","contentHash":"d5cc6fc2da153d579f50bd518d2af8b38449eb64b47be3d223bbabce837e4447"},"C:\\Users\\Julian\\Desktop\\Facultad\\Ingenieria en Software II\\GitHub Repos\\calculadora\\src\\Traducir.ts":{"path":"C:\\Users\\Julian\\Desktop\\Facultad\\Ingenieria en Software II\\GitHub Repos\\calculadora\\src\\Traducir.ts","statementMap":{"0":{"start":{"line":2,"column":0},"end":{"line":2,"column":62}},"1":{"start":{"line":3,"column":30},"end":{"line":23,"column":3}},"2":{"start":{"line":6,"column":4},"end":{"line":21,"column":6}},"3":{"start":{"line":7,"column":8},"end":{"line":7,"column":25}},"4":{"start":{"line":8,"column":8},"end":{"line":19,"column":9}},"5":{"start":{"line":9,"column":12},"end":{"line":9,"column":58}},"6":{"start":{"line":10,"column":12},"end":{"line":10,"column":58}},"7":{"start":{"line":11,"column":12},"end":{"line":11,"column":59}},"8":{"start":{"line":12,"column":12}